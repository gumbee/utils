{
  "name": "@gumbee/utils",
  "version": "1.0.0",
  "author": "Gumbee <mugeeb.hassan@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gumbee/utils.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.39.1",
    "@types/bun": "^1.3.4",
    "bunup": "^0.16.10",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^15.15.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "bugs": {
    "url": "https://github.com/gumbee/utils/issues"
  },
  "description": "Shared utilities for the Gumbee codebase",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://github.com/gumbee/utils#readme",
  "keywords": [
    "gumbee",
    "utils",
    "utilities",
    "typescript"
  ],
  "license": "MIT",
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "scripts": {
    "build": "bunup --dts",
    "dev": "bunup --watch --dts",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "changeset": "changeset",
    "version": "changeset version"
  },
  "sideEffects": false,
  "type": "module",
  "types": "./dist/index.d.ts"
}
