{
  "name": "@gumbee/utils",
  "version": "0.1.0",
  "description": "Shared utilities for the Gumbee codebase",
  "author": "Gumbee <hello@gumbee.io>",
  "license": "MIT",
  "homepage": "https://github.com/gumbee/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gumbee/utils.git"
  },
  "bugs": {
    "url": "https://github.com/gumbee/utils/issues"
  },
  "keywords": [
    "gumbee",
    "utils",
    "utilities",
    "typescript"
  ],
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bunup --dts",
    "dev": "bunup --watch --dts",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "prepublishOnly": "bun run build"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/bun": "^1.3.4",
    "bunup": "^0.16.10",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^15.15.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  }
}
